   ],
   "source": [
    "# Descargar datos de Yahoo Finance\n",
    "stocks = sorted(['KO', 'META', 'AMZN'])\n",
    "data = yf.download(stocks, start='2020-01-01', end='2023-10-01')['Adj Close']\n",
    "\n",
    "# Número de activos\n",
    "N=len(stocks)\n",
    "\n",
    "rf = 0.11\n",
    "\n",
    "# Calcular retornos diarios\n",
    "returns = data.pct_change().dropna()\n",
    "volatilities = returns.std() * (252**0.5)\n",
    "correlation_matrix = returns.corr()\n",
    "\n",
    "# Calcular la rentabilidad y volatilidad esperada para cada activo\n",
    "mean_returns = returns.mean() * 252\n",
    "cov_matrix = returns.cov() * 252\n",
    "\n",
    "def expected_return(weights, mean_returns):\n",
    "    return np.dot(mean_returns, weights)\n",
    "\n",
    "def var(weights,cov_matrix):\n",
    "    return weights.T.dot(cov_matrix).dot(weights)\n",
    "\n",
    "# Función para calcular la rentabilidad y volatilidad de un portafolio\n",
    "def portfolio_performance(weights, mean_returns, cov_matrix):\n",
    "    port_return = expected_return(weights, mean_returns)\n",
    "    port_volatility = np.sqrt(var(weights, cov_matrix))\n",
    "    return port_return, port_volatility\n",
    "\n",
    "# Función objetivo (minimizar la negación de Sharpe Ratio)\n",
    "def neg_sharpe_ratio(weights,mean_returns,cov_matrix,rf): \n",
    "    port_performance = portfolio_performance(weights, mean_returns, cov_matrix)\n",
    "    return -(port_performance[0]-rf) / port_performance[1]\n",
    "\n",
    "# Restricciones y límites\n",
    "constraints = ({'type': 'eq', 'fun': lambda weights: np.sum(weights) - 1})\n",
    "bounds = ((0,1),)*N\n",
    "\n",
    "# Crear pesos iniciales\n",
    "initial_weights=np.ones(N)/N\n",
    "\n",
    "# Max Ratio Sharpe\n",
    "max_ratio_sharpe = minimize(\n",
    "    fun=neg_sharpe_ratio,\n",
    "    x0=initial_weights,\n",
    "    args=(mean_returns,cov_matrix,rf),\n",
    "    bounds=bounds,\n",
    "    constraints=constraints\n",
    ")\n",
    "\n",
    "# Resultados\n",
    "weights_optimal = max_ratio_sharpe.x\n",
    "return_optimal = expected_return(weights_optimal, mean_returns)\n",
    "volatility_optimal = np.sqrt(var(weights_optimal, cov_matrix))\n",
    "\n",
    "# Definir un rango de volatilidades desde 0 hasta un valor máximo (por ejemplo, el doble de la volatilidad óptima)\n",
    "volatilities_range = np.linspace(0, 2*volatility_optimal, 100)\n",
    "\n",
    "# Calcular las rentabilidades de la CAL para ese rango de volatilidades\n",
    "cal_returns = rf + ((return_optimal - rf) / volatility_optimal) * volatilities_range\n",
    "\n",
    "# Datos estimados para Portafolio\n",
    "assets = {stock: [mean_returns[stock], volatilities[stock]] for stock in stocks}\n",
    "assets['Portafolio (max ratio Sharpe)'] = [return_optimal, volatility_optimal]\n",
    "\n",
    "# Calculando la frontera eficiente\n",
    "target_returns = np.linspace(mean_returns.min(), mean_returns.max(), 50)\n",
    "target_volatilities = []\n",
    "portfolio_weights = []\n",
    "\n",
    "for target in target_returns:\n",
    "    constraints = (\n",
    "        {'type': 'eq', 'fun': lambda x: portfolio_performance(x, mean_returns, cov_matrix)[0] - target},\n",
    "        {'type': 'eq', 'fun': lambda x: np.sum(x) - 1}\n",
    "    )\n",
    "    result = minimize(fun=var, x0=initial_weights, args=(cov_matrix), method='SLSQP', bounds=bounds, constraints=constraints)\n",
    "    target_volatilities.append(np.sqrt(result.fun))\n",
    "    portfolio_weights.append(result.x)\n",
    "\n",
    "# Crear texto para el hover\n",
    "hovertext = []\n",
    "for i in range(len(target_returns)):\n",
    "    hovertext.append('<br>'.join([f\"{stock}: {weight:.4f}\" for stock, weight in zip(stocks, portfolio_weights[i])]))\n",
    "\n",
    "# Crear una figura interactiva con Plotly\n",
    "fig = go.Figure()\n",
    "\n",
    "# Agregar acciones individuales y portafolio al gráfico\n",
    "for asset, values in assets.items():\n",
    "    fig.add_trace(go.Scatter(\n",
    "        x=[values[1]],\n",
    "        y=[values[0]],\n",
    "        mode='markers',\n",
    "        name=asset,\n",
    "        marker=dict(size=10, opacity=0.6)\n",
    "    ))\n",
    "\n",
    "# Agregar la frontera eficiente al gráfico\n",
    "fig.add_trace(go.Scatter(\n",
    "    x=target_volatilities,\n",
    "    y=target_returns,\n",
    "    mode='markers',\n",
    "    name='Frontera Eficiente',\n",
    "    marker=dict(\n",
    "        size=8, \n",
    "        opacity=0.5, \n",
    "        color=(target_returns-cetes_annual_return)/target_volatilities, \n",
    "        colorscale='Viridis', \n",
    "        showscale=True, \n",
    "        colorbar=dict(\n",
    "            x=1.05, \n",
    "            title=\"Ratio de Sharpe\"  # Título para la barra de colores\n",
    "        )\n",
    "    ),\n",
    "    text=hovertext,\n",
    "    hoverinfo=\"text+y+x\"\n",
    "))\n",
    "\n",
    "# Añadir la CAL a la figura de Plotly\n",
    "fig.add_trace(go.Scatter(\n",
    "    x=volatilities_range,\n",
    "    y=cal_returns,\n",
    "    mode='lines',\n",
    "    name='Capital Allocation Line',\n",
    "    line=dict(width=2, color='red')\n",
    "))\n",
    "\n",
    "# Establecer detalles del gráfico\n",
    "fig.update_layout(\n",
    "    title='Volatilidad vs. Rendimiento y Frontera Eficiente',\n",
    "    xaxis_title='Volatilidad',\n",
    "    yaxis_title='Retorno',\n",
    "    hovermode='closest',\n",
    "    legend=dict(x=0.5, y=-0.25, xanchor=\"center\", orientation=\"h\"), \n",
    "    margin=dict(r=30)\n",
    ")\n",
    "\n",
    "fig.show()"
   ]
  },
